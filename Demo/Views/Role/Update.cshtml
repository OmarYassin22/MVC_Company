@model RoleViewModel
@{
    ViewData["Title"] = "Update";
}

<h1>Update</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Update" enctype="multipart/form-data">
            <partial name="PartialViews/Create_Update" />
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="AddOrRemoveUser" asp-route-id="@Model.Id" value="" class="btn btn-primary">Add Or Remove User</a>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Validation {
    <partial name="_ValidationScriptsPartial" />

}
